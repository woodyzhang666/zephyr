/dts-v1/;

#include <wch/ch57x/ch573.dtsi>

/ {
	model = "ch573 evaluation board";
	compatible = "wch,ch573";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,console = &uart1;
	};
};

&clk_lsi {
	status = "okay";
};

&clk_lse {
	status = "okay";
};

&clk_hse {
	clock-frequency = <DT_FREQ_M(32)>;
	status = "okay";
};

&clk_pll {
	status = "okay";
};

&rcc {
	clocks = <&clk_pll>;
	clock-frequency = <DT_FREQ_M(20)>;
	status = "okay";
};

&uart1 {
	current-speed = <115200>;
	status = "okay";
};

